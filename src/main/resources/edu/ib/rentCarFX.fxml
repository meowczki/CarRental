<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.ib.RentCarController">
   <children>
      <ComboBox fx:id="carsCombo" layoutX="54.0" layoutY="77.0" prefHeight="26.0" prefWidth="292.0" />
      <Label layoutX="126.0" layoutY="115.0" text="szerokość geografczna" />
      <Label layoutX="133.0" layoutY="169.0" text="długość geografczna" />
      <TextField fx:id="latTV" layoutX="115.0" layoutY="131.0" />
      <TextField fx:id="lonTV" layoutX="115.0" layoutY="185.0" />
      <Button fx:id="rentBtn" layoutX="159.0" layoutY="232.0" mnemonicParsing="false" onAction="#rentOnClick" text="wypożycz" />
      <Button fx:id="returnBtn" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#returnBtnOnClick" text="wróć" />

   </children>
</AnchorPane>
